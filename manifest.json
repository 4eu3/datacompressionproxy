{
  "version": "1.0",
  "name": "Data Compression Proxy",
  "description": "Data Compression Proxy for Google Chrome on Desktop",
  "author": "niu tech",
  "permissions": ["webRequest", "webRequestBlocking", "declarativeWebRequest", "proxy", "tabs", "*://*/*"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "38": "off.png"
    },
    "default_title": "Data Compression Proxy: Disabled"
  },
  "manifest_version": 2
}
