{
  "version": "1.3",
  "name": "Data Compression Proxy",
  "description": "Data Compression Proxy for Google Chrome on Desktop",
  "author": "niu tech",
  "permissions": ["webRequest", "webRequestBlocking", "declarativeWebRequest", "proxy", "*://*/*"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "off.png",
    "default_title": "Data Compression Proxy: Disabled"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
